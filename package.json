{
  "name": "angular-quickstart",
  "version": "1.0.0",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"webpack --progress\" \"webpack-dev-server --inline --progress\"",
    "build": "webpack --progress",
    "build:prod": "webpack -p --config webpack.production.config.js --progress",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "serve": "webpack-dev-server --inline --progress"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "~2.0.2",
    "@angular/compiler": "~2.0.2",
    "@angular/core": "~2.0.2",
    "@angular/forms": "~2.0.2",
    "@angular/http": "~2.0.2",
    "@angular/platform-browser": "~2.0.2",
    "@angular/platform-browser-dynamic": "~2.0.2",
    "@angular/router": "~3.0.2",
    "@angular/upgrade": "~2.0.2",
    "angular-in-memory-web-api": "~0.1.5",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.39",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "concurrently": "^3.0.0",
    "typescript": "^2.0.3",
    "typings":"^1.4.0",
    "html-webpack-plugin": "^2.22.0",
    "node-sass": "^3.10.1",
    "raw-loader": "^0.5.1",
    "require-dir": "^0.3.0",
    "sass-loader": "^4.0.2",
    "ts-loader": "^0.8.2",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1",
    "webpack-md5-hash": "0.0.5"
  }
}

